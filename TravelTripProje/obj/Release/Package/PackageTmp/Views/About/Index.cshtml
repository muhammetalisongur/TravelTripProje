@using TravelTripProje.Models.Entity
@model List<Hakkimizda>
@{
    ViewBag.Title = "About";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content">
    <div class="wrap">
        <div class="about">
            <h3>Hakkimizda</h3>
            <p>
                @foreach (var item in Model)
                {
                    <img src="@item.FotoUrl" style="width:970px; height:350px" alt="" />
                    @item.Aciklama
                }
            </p>

        </div>
    </div>
</div>

